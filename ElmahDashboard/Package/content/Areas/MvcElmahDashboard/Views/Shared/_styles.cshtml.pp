@{
    Layout = null;
    Response.ContentType = "text/css";
    Response.Cache.SetExpires(DateTime.Now.AddDays(30));
}
HTML {
   -ms-overflow-style: scrollbar;
}
BODY {
    margin-top: 48px;
}
CANVAS {
  background-color: blue;
  width: 100%;
  height: auto;
}
.navbar {
    border: none;
    border-radius: 0;
}
#back-top {
	display: block;
    position: fixed;
    bottom: 15px;
    right: 15px;
    width:48px;
    height: 48px;
    padding: 9px 0px 0px 2px;
    font-size: 24px;
    font-weight: 400;
	color: #888;
    background-color: #bbb;
    opacity: 0.8;
	text-align: center;
	-webkit-border-radius: 15px;
	-moz-border-radius: 15px;
	border-radius: 15px;
}
#back-top:hover {
	color: #111;
    background-color: #777;
}
.column-filter {
    font-family: 'Arial Narrow', 'Gill Sans Condensed', 'Tahoma';
    font-size: 8pt;
}
.loader {
    width: 32px;
    height: 32px;
    display: inline-block;
}
.loader.loading {
    background-image: url('data:image/gif;base64,');
}
.floating-above {
    position: absolute;
    width: 300px;
}
.hourlabel TH {
    font-weight: normal;
    font-size: 8pt;
    text-align: center;
}
.hourvalue TD {
    font-weight: bold;
    text-align: center;
    font-size: 14pt;
    color: darkred;
}
.hourvalue0 {
    font-weight: bold;
    color: #fbb;
}
.hourvalue1 {
    font-weight: bold;
    color: #f88;
}
.hourvalue2 {
    font-weight: bold;
    color: #f66;
}
.hourvalue3 {
    font-weight: bold;
    color: #f00;
}
.lastvalue {
    font-size: 64pt;
    font-weight: bold;
    xfont-family: 'Calibri Light', sans-serif;
    color: darkred;
}
.legend {
    font-style: italic;
    text-align: center;
}
.unset .glyphicon {
    visibility: hidden;
}
#heartbeat-heart {
    font-size: 64px;
    color: green;
    animation-name: heartbeat-heart-animation;
    animation-duration: 5s;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
    -webkit-animation-name: heartbeat-heart-animation;
    -webkit-animation-duration: 5s;
    -webkit-animation-timing-function: linear;
    -webkit-animation-iteration-count: infinite;
}
@@keyframes heartbeat-heart-animation {
    0%   {color: green;}
    20%  {color: green;}
    90%  {color: lightgray;}
    100% {color: green;}
}
@@-webkit-keyframes heartbeat-heart-animation {
    0%   {color: green;}
    20%  {color: green;}
    90%  {color: lightgray;}
    100% {color: green;}
}
#heartbeat-exclamation {
    font-size: 64px;
    color: darkred;
    animation-name: heartbeat-exclamation-animation;
    animation-duration: 2s;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
    -webkit-animation-name: heartbeat-exclamation-animation;
    -webkit-animation-duration: 2s;
    -webkit-animation-timing-function: linear;
    -webkit-animation-iteration-count: infinite;
}
@@keyframes heartbeat-exclamation-animation {
    0%  {color: lightgray;}
    20%   {color: darkred;}
    100%   {color: darkred;}
}
@@-webkit-keyframes heartbeat-exclamation-animation {
    0%  {color: lightgray;}
    20%   {color: darkred;}
    100%   {color: darkred;}
}
.timebar {
    width: 0%;
    height: 2px;
    background-color: #d0d0d0;
    margin-top: 2px;
    margin-bottom: 4px;
}
@@keyframes timebar-animation {
    0%   {width: 100%;}
    100% {width: 0%;}
}
@@-webkit-keyframes timebar-animation {
    0%   {width: 100%;}
    100% {width: 0%;}
}
